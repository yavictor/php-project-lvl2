{
    "name": "yavictor/php_project2",
    "type": "project",
    "description": "cli config difference generator",
    "license": "MIT",
    "authors": [
      {
          "name": "yv",
          "email": "yavictor37@yandex.ru"
      }
    ],
    "bin": [
      "bin/gendiff"
    ],
    "autoload": {
        "psr-4": {
            "Php\\Package\\": "src",
            "Php\\Package\\Tests\\": "tests"
        },
        "files": [
          "src/DiffGenerator.php",
          "src/Parsers.php",
          "src/Parsers/JsonParser.php",
          "src/Parsers/YamlParser.php",
          "src/Formatters/Stylish.php",
          "src/Formatters/Plain.php",
          "src/Formatters/Json.php",
          "src/Formatters.php"
        ]
    },
    "scripts": {
      "phpcs": "phpcs -- --standard=PSR12 src bin tests",
      "test": "phpunit tests"
    },

    "require": {
      "php": ">=7.4",
      "docopt/docopt": "^1.0",
      "symfony/yaml": "^5.2",
      "funct/funct": "^1.5",
      "phpstan/extension-installer": "^1.1.0",
      "phpstan/phpstan": "^0.12.70"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "3.*",
        "hexlet/phpstan-fp": "^1.1"
    }
}
