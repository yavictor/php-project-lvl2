{
    "name": "hexlet/code",
    "description": "cli config difference generator",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Php\\Package\\": "src",
            "Php\\Package\\Tests\\": "tests"
        },
        "files": [
            "src/GenDiff.php",
            "src/Parsers/parsers.php"
        ]
    },
    "scripts": {
      "phpcs": "phpcs -- --standard=PSR12 src bin tests",
      "test": "phpunit tests"
    },
    "authors": [
        {
            "name": "yv",
            "email": "yavictor37@yandex.ru"
        }
    ],
    "require": {
      "php": ">=7.4",
      "docopt/docopt": "^1.0",
      "symfony/yaml": "^5.2",
      "funct/funct": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "3.*"
    }
}
